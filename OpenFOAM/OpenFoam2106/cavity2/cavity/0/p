FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            inletOutlet;
         inletValue           uniform 0;

        //type            fixedValue;
        //value           uniform 0;
    }

    symmetry
    {
        //type            symmetry;
                type            zeroGradient;

    }

    fixedWalls
    {
        type            zeroGradient;
    }

    potentialWall   // treated same as fixedWalls
    {
        //type            zeroGradient;
        type            fixedValue;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}


