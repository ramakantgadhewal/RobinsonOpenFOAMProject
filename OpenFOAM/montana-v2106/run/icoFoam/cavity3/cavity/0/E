FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      E;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
                // kg, m , s , K , mol , A, Light(?)
dimensions      [1 1 -3 0 0 -1 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        /*
        type            fixedValue;
        value           uniform (1 0 0);
        */
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

/*
    symmetry
    {
     //   type            symmetry;
             type            zeroGradient;

    }
*/
    fixedWalls
    {
        type            zeroGradient;
    }

    potentialWallTop // treated same as fixedWalls
    {
        type            zeroGradient;
    }

    potentialWallBot // treated same as fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


