FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       surfaceScalarField;
    location    "0.156";
    object      phiB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -2 0 0 -1 0];

oriented        oriented;

internalField   uniform 0.3;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 0.3;
    }
    right
    {
        type            calculated;
        value           uniform -0.3;
    }
    empty
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
}


