FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "200";
    object      phiB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -2 0 0 -1 0];
oriented        oriented;


internalField   uniform 600;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 600;
    }
    right
    {
        type            calculated;
        value           uniform -600;
    }
    empty
    {
        type            empty;
        value           nonuniform 0();
    }
}


